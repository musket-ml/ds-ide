<composite-editor
	xmlns="http://jetface.org/JetFace1.0/"
	caption="Please configure clazz labels">
	<model class="com.onpositive.musket.data.labels.LabelsSet" />

	<table bindTo="items" 
		enableDirectEdit="true">
		<columns>
			<column id="clazz" caption="Clazz" resizeWeight="1"
				editable="false" />
			<column id="label" caption="Label" resizeWeight="2"
				editable="true" />
		</columns>
	</table>
</composite-editor>